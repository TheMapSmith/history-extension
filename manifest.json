{
  "manifest_version": 3,
  "name": "Custom History Enhancements",
  "version": "1.0",
  "description": "Improves the behavior of Chrome's History tab",
  "permissions": ["tabs", "history"],
  "action": {
    "default_icon": "icon.png",
    "default_action": "background.js"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
